pipeline {
    agent any
    environment {
            CI = 'true'
        }

    stages {
        stage('Install Fronted') {
            steps {
                sh 'date'
                sh 'echo Iniciando instalación de depenencias en Fronted'

                sh  'cd /media/luismoroco/D/WORK/BlueCorner/fronted/'
                sh  'yarn install'
            }         
        }
        stage('Install Backend') {
            steps {
                sh 'date'
                sh 'echo Iniciando instalación de depenencias en Backend'

                sh  '/media/luismoroco/D/WORK/BlueCorner/backend/'
                sh  'yarn install'
            }         
        }
    }
}